(define (domain submarine)
    (:requirements
        :strips
    )

    (:predicates
        (subLoc ?x)
        (subDestination ?d)
    )
    
    (:action move
        :parameters
            (?x ?d)
        :precondition
            (and
                (subLoc ?x)
                (subDestination ?d)
            )
        :effect
            (and
                (not (subLoc ?x))
                (subLoc ?d)
            )
    )
)
