(define (problem simpleMovement)
  (:domain submarine)

  (:objects
      dock
      loc1
      loc2
      loc3
      loc4
      cap - captain
      bob - bridge
      nav - navigator
      artefact
      IAUV - IAUV
  )

  (:init
      (subLocation dock)
      (location loc1)
      (location loc2)
      (location loc3)
      (location loc4)
      (onBridge)
      (in cap bob)
      (in nav bob)
      (artefact artefact)
      (at artefact loc3)
      (deploy IAUV loc3)
      
  )

  (:goal
      (and
        (subLocation loc1)
        (picture artefact loc3)
      )
  )
)
